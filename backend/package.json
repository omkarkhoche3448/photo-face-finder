    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "image-size": "^1.1.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
